{
  "name": "template-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "CLOUDFLARE_ENV=local vite dev --port 3000",
    "build": "CLOUDFLARE_ENV=production vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "format": "biome format --fix",
    "lint": "biome lint --fix",
    "check": "biome check",
    "cf:deploy": "bun run build && wrangler deploy",
    "cf:typegen": "wrangler types -e local --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "db:generate": "drizzle-kit generate --config ./src/integrations/db/drizzle.config.ts",
    "db:local:migrate": "wrangler -e local d1 migrations apply db",
    "db:production:migrate": "wrangler -e production d1 migrations apply db",
    "db:local:studio": "ENV=local drizzle-kit studio --config ./src/integrations/db/drizzle.config.studio-local.ts",
    "db:production:studio": "ENV=production drizzle-kit studio --config ./src/integrations/db/drizzle.config.ts",
    "email:dev": "email dev -d ./src/integrations/emails/templates",
    "email:build": "email build -d ./src/integrations/emails/templates"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^1.20.1",
    "@faker-js/faker": "^10.2.0",
    "@orpc/client": "^1.13.4",
    "@orpc/json-schema": "^1.13.4",
    "@orpc/openapi": "^1.13.4",
    "@orpc/server": "^1.13.4",
    "@orpc/tanstack-query": "^1.13.4",
    "@orpc/zod": "^1.13.4",
    "@plunk/node": "^3.0.3",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@react-email/components": "^1.0.4",
    "@react-email/render": "2.0.0",
    "@t3-oss/env-core": "^0.13.10",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-devtools": "^0.7.11",
    "@tanstack/react-form": "^1.27.7",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router": "^1.147.3",
    "@tanstack/react-router-devtools": "^1.149.0",
    "@tanstack/react-router-ssr-query": "^1.147.3",
    "@tanstack/react-start": "^1.149.2",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/router-plugin": "^1.149.0",
    "@unpic/react": "^1.0.2",
    "better-auth": "^1.4.12",
    "better-sqlite3": "^12.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.544.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@nerdfolio/drizzle-d1-helpers": "^0.1.5",
    "@react-email/preview-server": "5.1.0",
    "@tanstack/devtools-vite": "^0.3.12",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.19.5",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "dotenv": "^16.6.1",
    "jsdom": "^27.4.0",
    "react-email": "5.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "ultracite": "6.4.2",
    "vite": "^7.3.1",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0",
    "wrangler": "^4.58.0"
  }
}
